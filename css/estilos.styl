/*Inicio estilos Productos*/
.ContentInfoProd
	display inline-block
	margin 2% 6%
	width 45%
	
	.DescripcionProd
		margin 1% auto
		width 90%
		
		figure
			height 269px
			width 513px
			
			img
				border-radius 12px
				width 100%
	
	.GraficoProd
		margin 73px 0 0 0
		
		figure
			height 370px
			margin 0 auto
			width 80%

.TituloCategorias2
	position absolute
	height 7em
	margin -785.8px 0px 0% 2.9%
	width 26.65%
	z-index 0
	
	figure
		height 7em
		position absolute
	
	div
		.ParrafoUno
			color white
			font-size 1.5em
			margin 10% 0px 0px 23%
