/*Inicio Estilos Contactenos*/
.ContentForm
	background url("../img/FondoContactenos.png")
	background-size 100%
	height 500px
	margin 20px auto 50px auto
	width 80%
	
	p
		font-size 22px
		padding 60px 0 0px 135px
	
	.Obligatorio
		color red
		font-size 13px
		padding 0px 0 0px 135px
	
	form
		margin 0 0 0 35px
		label
			display inline-block
			width 10%
			vertical-align top
		input
			border-top-right-radius 50px
			border-bottom-right-radius 50px
			display inline-block
		textarea
			border-top-right-radius 10px
			border-bottom-right-radius 10px
			display inline-block
			
		div
			background #5882AE
			border-top-right-radius 50px
			border-bottom-right-radius 50px
			margin 5px 0 0 -28px
			width 20%
			button
				background none
				border none
				border-left 1px solid white
				color white
				margin 2px 0 2px 135px
				
			:hover
				cursor pointer
	
/*Fin Etilos Contactenos*/

/*Inicio estilos footer*/
footer
	height 300px
	width 100%
	
	.Partners
		border-top 1px dashed #FF5000
		margin 1% auto
		
		width 90%
		
		figure
			display inline-block
			margin 0px 40px 20px
			img
				width 100%
		
		p
			color #E35000
			font-size 1.5em
		
		div
			overflow auto
			width 100%
			
			div
				width 143%
			
		.Partner1
			width 230px
		.Partner2
			width 140px
		.Partner3
			width 140px
		.Partner4
			width 230px
	.Copy
		background none repeat scroll 0% 0% rgb(227, 80, 0)
		height 15%
		margin 1% 0px 0px
		width 100%
		
		p
			color white
			font-size 15px
			padding 1% 0px 0px 1%
/*Fin estilos footer*/

